{
    "compilerOptions": {
        "rootDir": "src/",
        "noImplicitAny": true,
        "removeComments": true,
        "sourceMap": true,
        //"outFile": "publish-subscribe-library.js",
        "target": "es5"
    },
    // This specifies the files and order to compile the files in.
